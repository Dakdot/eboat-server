generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DataReading {
  id                  Int   @id @default(autoincrement())
  voltage             Float
  throttle_mv         Float
  throttle_percentage Float
  rpm                 Int
  torque              Int
  motor_temp          Float
  current             Float
  power               Int
  pitch               Float
  yaw                 Float
  roll                Float
  ax                  Float
  ay                  Float
  az                  Float
  heading             Int
}
